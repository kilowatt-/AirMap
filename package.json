{
  "name": "airmap",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "cd server && node index.js",
    "client": "cd client && npm start",
    "server": "cd server && nodemon index.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install && cd .. && npm install --only=dev --no-shrinkwrap && npm run build "
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "flatted": "^2.0.2",
    "node-env-run": "^3.0.2",
    "nodemon": "^2.0.2"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
